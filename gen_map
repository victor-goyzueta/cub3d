python3 - << 'EOF' > killer.cub
size = 4242

print("NO ./textures/quake/no.xpm")
print("SO ./textures/quake/so.xpm")
print("WE ./textures/quake/we.xpm")
print("EA ./textures/quake/ea.xpm")
print()
print("F 100,100,100")
print("C 50,50,50")
print()

print("1" * size)
for y in range(1, size - 1):
    if y == size // 2:
        print("1" + "0" * (size // 2 - 1) + "N" + "0" * (size // 2 - 1) + "1")
    else:
        print("1" + "0" * (size - 2) + "1")
print("1" * size)
EOF
